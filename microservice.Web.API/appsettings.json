{
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "sqlCon": "Data Source=HAKAM\\SQLEXPRESS;Initial Catalog=AppointmentsContext;Integrated Security=True"
  },
  "API": {
    "configuration-microservice": "https://localhost:7063/api",
    "users-microservice": "https://localhost:7062/api"
  }

}
